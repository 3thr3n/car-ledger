{
  "openapi" : "3.0.3",
  "info" : {
    "title" : "pump-pal API",
    "version" : "0.1.0-SNAPSHOT"
  },
  "servers" : [ {
    "url" : "http://localhost:8080",
    "description" : "Auto generated value"
  }, {
    "url" : "http://0.0.0.0:8080",
    "description" : "Auto generated value"
  } ],
  "paths" : {
    "/api/auth/callback" : {
      "get" : {
        "tags" : [ "Auth Resource" ],
        "description" : "This only for redirect purposes of oauth!",
        "operationId" : "callback",
        "responses" : {
          "200" : {
            "description" : "OK"
          }
        }
      }
    },
    "/api/auth/login" : {
      "get" : {
        "tags" : [ "Auth Resource" ],
        "description" : "Here should the browser redirect, when 'login' is pressed",
        "operationId" : "login",
        "responses" : {
          "200" : {
            "description" : "OK",
            "content" : {
              "text/plain" : {
                "schema" : {
                  "type" : "string"
                }
              }
            }
          }
        }
      }
    },
    "/api/bill/{carId}" : {
      "put" : {
        "tags" : [ "Bill Resource" ],
        "operationId" : "AddNewBill",
        "parameters" : [ {
          "name" : "carId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "format" : "int64",
            "type" : "integer"
          }
        } ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/BillInputPojo"
              }
            }
          }
        },
        "responses" : {
          "200" : {
            "description" : "Bill created."
          },
          "400" : {
            "description" : "Car is not for your user."
          },
          "500" : {
            "description" : "Something went wrong while saving. Please ask the server admin for help."
          }
        }
      }
    },
    "/api/bill/{carId}/all" : {
      "get" : {
        "tags" : [ "Bill Resource" ],
        "description" : "Gets all bills for specified car",
        "operationId" : "getAllBills",
        "parameters" : [ {
          "name" : "carId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "format" : "int64",
            "type" : "integer"
          }
        }, {
          "name" : "page",
          "in" : "query",
          "schema" : {
            "format" : "int32",
            "default" : 1,
            "type" : "integer"
          }
        }, {
          "name" : "size",
          "in" : "query",
          "schema" : {
            "format" : "int32",
            "default" : 10,
            "type" : "integer"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Bills found.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/BillPojoPaged"
                }
              }
            }
          }
        }
      }
    },
    "/api/bill/{carId}/{billId}" : {
      "delete" : {
        "tags" : [ "Bill Resource" ],
        "operationId" : "deleteBill",
        "parameters" : [ {
          "name" : "billId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "format" : "int64",
            "type" : "integer"
          }
        }, {
          "name" : "carId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "format" : "int64",
            "type" : "integer"
          }
        } ],
        "responses" : {
          "202" : {
            "description" : "Bill deleted."
          },
          "400" : {
            "description" : "Bill not found on the specified car."
          },
          "500" : {
            "description" : "Something went wrong while deleting. Please ask the server admin for help."
          }
        }
      }
    },
    "/api/car/my" : {
      "get" : {
        "tags" : [ "Car Resource" ],
        "operationId" : "getMyCars",
        "responses" : {
          "200" : {
            "description" : "Get all cars.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/CarPojo"
                  }
                }
              }
            }
          }
        }
      },
      "put" : {
        "tags" : [ "Car Resource" ],
        "operationId" : "createCar",
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/CarInputPojo"
              }
            }
          }
        },
        "responses" : {
          "202" : {
            "description" : "Car created."
          },
          "400" : {
            "description" : "Maximal amount of cars created."
          },
          "500" : {
            "description" : "Something went wrong while saving. Please ask the server admin for help."
          }
        }
      }
    },
    "/api/car/my/{id}" : {
      "get" : {
        "tags" : [ "Car Resource" ],
        "operationId" : "getMyCar",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "required" : true,
          "schema" : {
            "format" : "int64",
            "type" : "integer"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Car found.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/CarPojo"
                }
              }
            }
          },
          "204" : {
            "description" : "Id was not found."
          }
        }
      }
    },
    "/api/import/{carId}" : {
      "post" : {
        "tags" : [ "Import Resource" ],
        "description" : "\tThis is the description for the import of an csv of your bills.\n\n\tYou need to add the csv and optionally the order in the csv (starts with 0).\n\tIf you're not adding the order, the default is: day, unit, pricePerUnit, distance, estimate\n\tSeperator between columns is ',' (comma)\n",
        "operationId" : "ImportCsv",
        "parameters" : [ {
          "name" : "carId",
          "in" : "path",
          "required" : true,
          "schema" : {
            "format" : "int64",
            "type" : "integer"
          }
        }, {
          "name" : "skipHeader",
          "in" : "query",
          "schema" : {
            "type" : "boolean"
          }
        } ],
        "requestBody" : {
          "content" : {
            "multipart/form-data" : {
              "schema" : {
                "type" : "object",
                "properties" : {
                  "file" : {
                    "format" : "binary",
                    "type" : "string"
                  },
                  "order" : {
                    "$ref" : "#/components/schemas/CsvOrder"
                  }
                }
              },
              "encoding" : {
                "file" : {
                  "contentType" : "text/csv"
                },
                "order" : {
                  "contentType" : "application/json"
                }
              }
            }
          }
        },
        "responses" : {
          "202" : {
            "description" : "CSV was imported."
          },
          "400" : {
            "description" : "- Order is invalid\n- Car not found"
          },
          "500" : {
            "description" : "- CSV was not set\n- Something went wrong while importing. Please ask the server admin for help."
          }
        }
      }
    },
    "/api/user/me" : {
      "get" : {
        "tags" : [ "Account Resource" ],
        "operationId" : "getMyself",
        "responses" : {
          "200" : {
            "description" : "User found.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/AccountPojo"
                }
              }
            }
          }
        }
      }
    }
  },
  "components" : {
    "schemas" : {
      "AccountPojo" : {
        "type" : "object",
        "properties" : {
          "maxCars" : {
            "format" : "int32",
            "type" : "integer"
          },
          "name" : {
            "type" : "string"
          }
        }
      },
      "BillInputPojo" : {
        "type" : "object",
        "properties" : {
          "day" : {
            "$ref" : "#/components/schemas/LocalDate"
          },
          "distance" : {
            "type" : "number"
          },
          "unit" : {
            "format" : "double",
            "type" : "number"
          },
          "pricePerUnit" : {
            "format" : "double",
            "type" : "number"
          },
          "estimate" : {
            "format" : "double",
            "type" : "number"
          }
        }
      },
      "BillPojo" : {
        "type" : "object",
        "properties" : {
          "id" : {
            "format" : "int64",
            "type" : "integer"
          },
          "day" : {
            "$ref" : "#/components/schemas/LocalDate"
          },
          "distance" : {
            "type" : "number"
          },
          "unit" : {
            "type" : "number"
          },
          "pricePerUnit" : {
            "type" : "number"
          },
          "estimate" : {
            "type" : "number"
          },
          "calculated" : {
            "type" : "number"
          },
          "calculatedPrice" : {
            "type" : "number"
          }
        }
      },
      "BillPojoPaged" : {
        "type" : "object",
        "properties" : {
          "total" : {
            "format" : "int64",
            "type" : "integer"
          },
          "page" : {
            "format" : "int32",
            "type" : "integer"
          },
          "size" : {
            "format" : "int32",
            "type" : "integer"
          },
          "data" : {
            "type" : "array",
            "items" : {
              "$ref" : "#/components/schemas/BillPojo"
            }
          }
        }
      },
      "CarInputPojo" : {
        "type" : "object",
        "properties" : {
          "description" : {
            "type" : "string"
          }
        }
      },
      "CarPojo" : {
        "type" : "object",
        "properties" : {
          "id" : {
            "format" : "int64",
            "type" : "integer"
          },
          "description" : {
            "type" : "string"
          },
          "amountBills" : {
            "format" : "int32",
            "type" : "integer"
          }
        }
      },
      "CsvOrder" : {
        "type" : "object",
        "properties" : {
          "day" : {
            "format" : "int32",
            "type" : "integer"
          },
          "unit" : {
            "format" : "int32",
            "type" : "integer"
          },
          "pricePerUnit" : {
            "format" : "int32",
            "type" : "integer"
          },
          "distance" : {
            "format" : "int32",
            "type" : "integer"
          },
          "estimate" : {
            "format" : "int32",
            "type" : "integer"
          }
        }
      },
      "LocalDate" : {
        "format" : "date",
        "type" : "string",
        "example" : "2022-03-10"
      }
    },
    "securitySchemes" : {
      "SecurityScheme" : {
        "type" : "openIdConnect",
        "description" : "Authentication",
        "openIdConnectUrl" : "http://192.168.0.5:8085/realms/pump-pal/.well-known/openid-configuration"
      }
    }
  }
}